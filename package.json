{
  "name": "myapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.1",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "svelte-language-server": "^0.16.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "typescript-language-server": "^4.3.3",
    "vite": "^4.2.0"
  },
  "type": "module",
  "dependencies": {
    "flowbite-svelte": "^0.34.2"
  },
  "prettier": {
    "printWidth": 100,
    "plugins": [
      "prettier-plugin-svelte"
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ]
  }
}
